<section class="top-ten__list-container">
  <h2 class="top-ten__header"> Top <%= category %></h2>
  <% Work.top_ten(category).each do |work| %>
    <ul class="list-group top-ten__list">
      <li class="list-group-item">
        <h4>
          <%= link_to work.title, work_path(work) %>
          <small class="top_ten__creator"> by <%= work.creator %></small>
          <!--    TODO this is id for now!-->
        </h4>
        <p><%= work.votes_count %> votes</p>
      </li>
    </ul>
  <% end %>
</section>